generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model FormConfig {
  id               String @id @default(auto()) @map("_id") @db.ObjectId
  ReadableDateTime String
  formClose        String
}

model InvitedUser {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model OverrideAccept {
  id     String  @id @default(auto()) @map("_id") @db.ObjectId
  status Boolean
}

model Wishes {
  id            String  @id @default(auto()) @map("_id") @db.ObjectId
  Bsidepicture      String
  DMY           String
  agreed        String
  approved      Boolean
  comment       String
  count         Int
  giftId        String?
  lowercasename String
  name          String
  picture       String
  sidepicture   String
  stattime      String
  time          String
}
